.skill
  &__name 
    margin-bottom 0 
    font-size 1.28rem 
    font-weight normal 
    text-transform uppercase 
    text-align center 

  &__level 
    text-align center 
    margin-top 0.35rem 
    padding 0 

  &__point
    border 0.57rem solid mainGray 
    display inline-block 
    border-radius 100% 
    margin-right 0.5% 
    
    &_filled
      border 0.56rem solid  mainBlue 

    &:last-child
      margin-right 0 