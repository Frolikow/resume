.contact
  margin-bottom 2rem
  margin-top 0.9375rem
  text-align left

  &__data-title, &__data-value
    display inline-block
    margin 0

  &__data-title
    color mainBlue 
    text-transform uppercase 
    font-weight bold
    font-size 1.375rem
    line-height 3.375rem
    width 29%
    padding-left 0.3125rem

  &__data-value
    width 60%
    color mainBlack 
    text-decoration none
    font-size 1.25rem
    letter-spacing 0.0187rem