.contact
  margin-top 1rem
  margin-bottom 1rem
  text-align left

  &__data-name, &__data-value
    display inline-block
    height 2rem
    margin 0
    
  &__data-name
    color mainBlue 
    text-transform uppercase 
    font-weight bold
    width 29%
    min-width 3rem
    max-width 10rem
  &__data-value
    width 70%
    min-width 5rem
    color mainBlack 
    text-decoration none